using System;

namespace ะก__
{
    class Program
    {
        static void Main(string[] args) 
        {
            Console.Write("Enter x ");
            int x = int.Parse(Console.ReadLine());
            Console.Write("Enter a ");
            int a = int.Parse(Console.ReadLine());
            int b = x % 10 + x / 10;
            Console.WriteLine("{0}", (b % 10 == a) ? "YES" : "NO");
        }
    }
}

